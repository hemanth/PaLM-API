{
    "builds": [
        {
            "src": "api/server.py",
             "config": {
                 "includeFiles": ["api/prompts.py"]
             }
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/server.py"
        }
    ]
}
